version: '3.8'

services:
  nome-servico-cliente:
    image: debian:latest
    container_name: nome-container-cliente
    stdin_open: true
    tty: true
    command: tail -f /dev/null
   
    restart: unless-stopped
    environment:
      - TZ=America/Sao_Paulo
      - USERID=1000
      - GROUPID=1000
